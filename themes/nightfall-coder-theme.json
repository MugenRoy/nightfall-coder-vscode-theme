  "tokenColors": [
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
          "foreground": "#6D6D6D"
      }
    },
    {
      "name": "String",
      "scope": ["string", 
                "meta.embedded.assembly",
                "string.tag",
                "string.value",
                ],
      "settings": {
        "foreground": "#E4B1F0"
      }
    },
    {
      "name": "Keyword Control and Other",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#83D6C5",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keyword Operator",
      "scope": "keyword.operator",
      "settings": {
          "foreground": "#89B4FA"
      }
    },
    {
      "name": "Function Declarations",
      "scope": [
          "entity.name.function",
          "support.function",
          "support.constant.handlebars",
          "source.powershell variable.other.member",
          "entity.name.operator.custom-literal",
          "entity.source.cmake",
          "constant.character.entity"
      ],
      "settings": {
          "foreground": "#AAA0FA"
      }
    },
    {
      "name": "Storage",
      "scope": "storage.type, storage.modifier, storage.control",
      "settings": {
        "foreground": "#83D6C5",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric, keyword.control.directive",
      "settings": {
        "foreground": "#DEC76E",
        "fontStyle": ""
      }
    },
    {
      "name": "Types declaration and references",
      "scope": [
          "support.class",
          "support.type",
          "entity.name.type",
          "entity.name.namespace",
          "entity.other.attribute",
          "entity.name.scope-resolution",
          "entity.name.class",
          "storage.type.numeric.go",
          "storage.type.byte.go",
          "storage.type.boolean.go",
          "storage.type.string.go",
          "storage.type.uintptr.go",
          "storage.type.error.go",
          "storage.type.rune.go",
          "storage.type.cs",
          "storage.type.generic.cs",
          "storage.type.modifier.cs",
          "storage.type.variable.cs",
          "storage.type.annotation.java",
          "storage.type.generic.java",
          "storage.type.java",
          "storage.type.object.array.java",
          "storage.type.primitive.array.java",
          "storage.type.primitive.java",
          "storage.type.token.java",
          "storage.type.groovy",
          "storage.type.annotation.groovy",
          "storage.type.parameters.groovy",
          "storage.type.generic.groovy",
          "storage.type.object.array.groovy",
          "storage.type.primitive.array.groovy",
          "storage.type.primitive.groovy",
          "storage.modifier.import.java",
          "storage.modifier.package.java"
      ],
      "settings": {
          "foreground": "#d7d7d7ca",
          "fontStyle": ""
      }
    },
    {
      "name": "Constant String",
      "scope": "constant.language",
      "settings": {
        "foreground": "#83D6C5"
      }
    },
    // JSON Start
    {
      "name": "JSON Key - Level 0",
      "scope": [
        "meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#AAA0FA"
      }
    },
    {
      "name": "JSON Key - Level 1",
      "scope": [
        "meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#a9b1d6"
      }
    },
    {
      "name": "JSON Constant Numeric",
      "scope": "constant.numeric.json",
      "settings": {
        "foreground": "#DEC76E"
      }
    },
    // JSON End
    // MARKDOWN Start
    {
      "name": "Markdown - Underline",
      "scope": [
        "markup.underline"
      ],
      "settings": {
        "foreground": "#83D6C5",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markdown - Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#a3d78b",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown - Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    }, 	
    {
      "name": "Markdown - Strikethrough",
      "scope": [
        "markup.strikethrough"
      ],
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "name": "Markdown - Blockquote",
      "scope": "punctuation.definition.quote.begin.markdown",
      "settings": {
          "foreground": "#ffc2e29e"
      }
    },
    {
      "name": "Markdown - Heading",
      "scope": [
        "markup.heading.markdown",
      ],
      "settings": {
        "foreground": "#AAA0FA"
      }
    },
    {
      "name": "Markdown - Code Block",
      "scope": [
        "markup.fenced_code.block.markdown punctuation.definition.markdown"
      ],
      "settings": {
        "foreground": "#ffc2e29e"
      }
    },
    // MARKDOWN End
    // MARKUP Diff
    {
      "name": "Diff - inserted",
      "scope": "markup.inserted",
      "settings": {
          "foreground": "#2EA043"
      }
    },
    {
      "name": "Diff - Deleted",
      "scope": "markup.deleted, invalid",
      "settings": {
          "foreground": "#db4b4b"
      }
    },
    {
        "name": "Diff - Changed",
        "scope": "markup.changed",
        "settings": {
            "foreground": "#89B4FA"
        }
    },
    // MARKUP Diff End
    // HTML Start
    {
      "name": "Brackets of XML/HTML tags",
      "scope": "punctuation.definition.tag",
      "settings": {
          "foreground": "#d7d7d7ca"
      }
    },
    {
      "name": "HTML Tag Inner", // Works for yaml as well
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#83D6C5"
      }
    },
    {
      "name": "HTML Attribute Name",
      "scope": "entity.other.attribute-name, support.type.property-name",
      "settings": {
          "foreground": "#AAA0FA"
      }
    },
    // HTML End
    // CSS Start 
    {
      "name": "CSS Tag",
      "scope": "entity.name.tag.css, entity.other.attribute-name.class.css",
      "settings": {
        "foreground": "#89B4FA"
      }
    },
    {
      "name": "CSS Id",
      "scope": "entity.other.attribute-name.id",
      "settings": {
        "foreground": "#DEC76E"
      }
    },
    // CSS End
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "newOperator": "#83D6C5",
    "stringLiteral": "#E4B1F0",
    "customLiteral": "#AAA0FA",
    "numberLiteral": "#DEC76E" // #DEC76E #7ccb72c
  }
}